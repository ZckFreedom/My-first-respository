def fibonacci( number ):
    if number==0:return 0
    elif number==1:return 1
    else :return fibonacci(number-1) + fibonacci(number-2)

#user_input = int (input("Enter the number that you want:"))
#user_fibonacci = fibonacci(user_input)
user_array = int(input("Enter the number you want now:"))
int_fibonacci = []
for i in range(user_array):
    int_fibonacci.append( fibonacci(i) )
print (int_fibonacci )
#print( user_fibonacci)
